[nginx-teapot]
enabled     = true
ignoreip    = 127.0.0.0/8, 192.168.0.0/16
maxretry    = 2
findtime    = 1d
bantime     = 1w
logpath     = /var/log/nginx/access.log
filter      = nginx-teapot

cfemail     = # CF account e-mail
cfapikey    = # CF API Key
action      = nftables[type=allports]
              #cloudflare[cfuser="%(cfemail)s", cftoken="%(cfapikey)s"]
