[filter "npmrc"]
    smudge = cat
    clean = "sed -E '/^[@/]/d'"

[filter "sshconfig"]
    smudge = cat
    clean = "sed -E '/^#|^Host [^*]+|^ |^$/d'"

[filter "htoprc"]
    smudge = cat
    clean = "sed -E '/#|sort_key|tree_view/d'"

[filter "vnstat"]
    smudge = cat
    clean = "sed -E '/Interface/s/\"[^\"]*\"/\"eth0\"/'"
